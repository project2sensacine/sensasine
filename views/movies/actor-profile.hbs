<div class="container">
  <div class="row">
    <div class="col-md-4">
      <figure class="profile">
        <img src="https://image.tmdb.org/t/p/w276_and_h350_face{{profile_path}}" alt="{{name}}">
      </figure>
    </div>
    <div class="col-md-6">
      <h2>{{name}}</h2>
      <h5>Nacimiento: {{birthday}}</h5>
      <p>{{biography}}</p>

    </div>
  </div>
</div>